<!DOCTYPE html>
<html lang="en">
<head>
    <title>TinyApp</title>
</head>
<body>
  <header>
    <% include partials/_header %>
  </header>
  <% if (username === 'guest') { %>

  <% } else { %>
  <ul>
      <% for (url in user_id) { %>
          <li>
            <form action= <%= `/urls/${url}/delete` %> method="POST" style="margin:150px;">
                <label> <%= url %> <%= user_id[url] %> </label>
                <a href= "/urls/<%=url%>"> edit </a>
                <input type="submit" value="Delete">
            </form>
          </li>
      <% } %>
  </ul>
  <% } %>
</body>
</html>
